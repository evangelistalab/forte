#! L-CTSD/STO-3G energy for a LiF molecule.
#! This tests CT frozen core energy.
#! The reference value is from a spin-integrated code.
#! Generated using commit GITCOMMIT

import forte

refscf = -105.359494997675554 #TEST
refct  = -105.418209109638468 #TEST

molecule {
0 1
Li
F 1 1.588476
}

set {
  reference rhf
  basis sto-3g
  e_convergence 12
  freeze_core true
  scf_type pk
  df_scf_guess false
}

set forte{
  job_type tensorsrg
  srg_mode ct
}

energy('scf')
compare_values(refscf, get_variable("SCF total energy"),10, "SCF energy") #TEST
energy('forte')
compare_values(refct, get_variable("CURRENT ENERGY"),10, "SR-CTSD energy") #TEST
