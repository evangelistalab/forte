# HF, RHF canonical orbitals, CAS-CI(2,2)


import libadaptive

refscf = -99.983408932712
reflci = -100.0470998238599

molecule hf{
   F
   H 1 1.732878731527
units bohr
}

set {
  basis 6-31G
  e_convergence 10
  d_convergence 10
  r_convergence 10
}

set libadaptive {
  det_threshold 100.00
  den_threshold 100.00
  energy_type full
  restricted_docc [2,0,0,0]
  active          [3,0,2,2]
  charge 0
  nroot 6
}

energy('mcscf')
energy('libadaptive')

compare_values(refscf, get_variable("SCF total energy"),10, "SCF energy") #TEST
compare_values(reflci, get_variable("LAMBDA-CI ENERGY"),10, "Lambda-CI energy") #TEST
