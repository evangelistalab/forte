#! RHF-SR-LCTSD DZ energy for the H2O molecule, with Z matrix input.
#! Reference energy from a spin-orbit pilot code


import libadaptive

refscf = -76.005679425830 #TEST
refct  = -76.158509610358138 #TEST
molecule {
0 1
O
H 1 1.0
H 1 1.0 2 104.5
}

set {
  basis DZ
  reference rhf
  e_convergence 12
}

set libadaptive {
  det_threshold 0.01
  maxiter 50
}

energy('sr-lctsd')

compare_values(refscf, get_variable("SCF total energy"),10, "SCF energy") #TEST
compare_values(refct, get_variable("CURRENT ENERGY"),10, "SR-CTSD energy") #TEST
