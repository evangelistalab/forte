
import libadaptive

refscf  = -108.231992299023318 #TEST
refdsrg = -108.433328203370323 # -108.433785371452998 #TEST

molecule {
0 1
N
N 1 3.0
}

set {
  basis cc-pVDZ
  reference rhf
  e_convergence 12
}

set libadaptive {
  srg_comm fo
  dsrg_s 0.1
  maxiter 100
}

energy('ct')

compare_values(refscf, get_variable("SCF total energy"),10, "SCF energy") #TEST
compare_values(refdsrg, get_variable("CURRENT ENERGY"),10, "DSRGSD energy") #TEST
