#! Generated using commit GITCOMMIT 
# Basic ACI calculation with energy threshold selection

import forte

refscf = -14.84351701863007 
refaci = -14.880501440898 #-14.880501440889
refacipt2 = -14.880559933778 #-14.880559935217

molecule li2{
  Li
  Li 1 3.7794522658
  units bohr
}

set {
  basis cc-pcvtz
  e_convergence 10
  d_convergence 10
  r_convergence 10
  scf_type pk
  reference rhf
  docc = [2,0,0,0,0,1,0,0]
}

set scf {
}

set forte {
  active_space_solver aci
  frozen_docc [1,0,0,0,0,1,0,0]
  diag_algorithm sparse
  sigma 0.0015
  gamma 0.0015
  nroot 1
  charge 0
  aci_enforce_spin_complete true
}

E, wfn = energy('scf', return_wfn=True)

compare_values(refscf, variable("CURRENT ENERGY"),9,"SCF energy")

energy('forte', ref_wfn=wfn)

compare_values(refaci, variable("ACI ENERGY"),9,"ACI energy")
compare_values(refacipt2, variable("ACI+PT2 ENERGY"),9,"ACI+PT2 energy")

