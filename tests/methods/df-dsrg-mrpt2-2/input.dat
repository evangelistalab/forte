#! Performs a DF-DSRG-MRPT2 on HF.
#! Generated using commit GITCOMMIT
import forte

refmcscf      = -99.87285077494816
refdsrgpt2    = -100.107827239125655

memory 500 mb

molecule HF{
  0 1
  F
  H  1 R
  R = 1.50
}

set globals{
   basis                   cc-pvdz
   df_basis_mp2            cc-pvdz-ri
   reference               RHF
   scf_type                DF
   d_convergence           12
   e_convergence           15
}

set forte{
   frozen_docc            [1,0,0,0]
   restricted_docc        [1,0,1,1]
   active                 [2,0,0,0]
   root_sym                0
   nroot                   1
   dsrg_s                  1.0
   int_type                df
   job_type                three-dsrg-mrpt2
}

ref = energy('scf')
energy('forte')

compare_values(refmcscf, ref,10,"SCF energy")
compare_values(refdsrgpt2, get_variable("CURRENT ENERGY"),10,"DSRG-MRPT2 energy")
