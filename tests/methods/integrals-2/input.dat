#! Test cholesky integrals (tight convergence) with frozen core and virtuals

import forte

refscf = -112.67064846447627
reffci = -112.74446815362198

molecule {
0 1
C
O 1 R
R = 2.5
units bohr
}

set {
  basis cc-pVDZ
  scf_type pk
  e_convergence 14
}

set forte {
  active_space_solver fci
  int_type cholesky
  cholesky_tolerance 1.0e-12
  frozen_docc      [2 ,0 ,0 ,0]
  restricted_docc  [2 ,0 ,0 ,0]
  active           [2 ,2 ,2 ,2]
  e_convergence 12
  mcscf_reference false
}

energy('forte')
compare_values(refscf, variable("SCF TOTAL ENERGY"),10, "SCF energy") #TEST
compare_values(reffci, variable("CURRENT ENERGY"),10, "FCI energy") #TEST
