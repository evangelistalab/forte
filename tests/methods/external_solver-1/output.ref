import forte

refe = -1.159592141760552

molecule {
0 1
H
H 1 0.7

}

set {
  basis              cc-pvdz
  scf_type           pk
  e_convergence      12
}

set forte {
  job_type             newdriver
  active_space_solver  external
  read_wfn             true
  correlation_solver   sa-mrdsrg
  corr_level           ldsrg2
  e_convergence        8
  active               [1, 0, 0, 0, 0, 1, 0, 0]
  restricted_docc      [0, 0, 0, 0, 0, 0, 0, 0]
  dsrg_s               0.5
}

energy('forte')
compare_values(refe, variable("CURRENT ENERGY"),9, "LDSRG(2) energy (external RDM)")
