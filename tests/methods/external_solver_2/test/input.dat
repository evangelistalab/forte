import forte

molecule {
0 1
H
H 1 0.75
}

set {
  basis dz
  scf_type pk
  e_convergence 12
}

set forte {
  active_space_solver fci
  correlation_solver dsrg-mrpt2
  relax_ref once
  active [1,0,0,0,0,1,0,0]
  restricted_docc [0,0,0,0,0,0,0,0]
  threepdc zero
}

