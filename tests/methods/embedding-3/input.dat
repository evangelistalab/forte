H        0.2749376338      3.2174213526      0.3670138598

     units angstrom
     no_reorient
     symmetry c1
}

set {
  basis                  sto-3g
  reference              rhf
  scf_type               pk
  maxiter                200
  e_convergence          10
  d_convergence          8
  docc                   [12]
  freeze_core            true
  frozen_docc            [3]
