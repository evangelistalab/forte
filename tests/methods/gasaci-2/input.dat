refacipt2 = -14.5181839579

molecule li2{
   Li
   Li 1 1.0000
}

set {
  basis DZ
  e_convergence 10
  d_convergence 10
  r_convergence 6
}

set scf {
  scf_type pk
  reference rhf
  docc = [2,0,0,0,0,1,0,0]
