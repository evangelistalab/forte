#! L-CTSD DZ energy for a H4 molecule.  This tests CT without symmetry.
#! The reference value is from a spin-integrated code.


import libadaptive

refscf = -2.00742527761572 #TEST
refct  = -2.069601874659799 #TEST

molecule hf {
0 1
H -0.4  0.0 0.0
H  0.4  0.0 0.0
H  0.1 -0.3 1.0
H -0.1  0.5 1.0
}

set {
  reference rhf
  e_convergence 12
  freeze_core false
  scf_type pk
  df_scf_guess false
}

set libadaptive {
  dsrg_s 1.0  
}

set basis DZ
energy('ct')

compare_values(refscf, get_variable("SCF total energy"),10, "SCF energy") #TEST
compare_values(refct, get_variable("CURRENT ENERGY"),10, "SR-CTSD energy") #TEST
